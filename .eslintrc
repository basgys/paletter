{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "semi": 2,
    "no-console": 0
  },
  "env": {
    "node": true
  },
  "globals": {
    "describe": true,
    "it": true,
    "expect": true,
    "jest": true
  }
}
